{
  "name": "scrollto-with-animation",
  "version": "1.7.0",
  "description": "Animated scrolling without any dependency on libraries",
  "main": "src/scrollto-with-animation.js",
  "scripts": {
    "open": "chromium-browser index.html",
    "dev": "npm run watch && npm run open",
    "watch": "watchify -t babelify src/AnimatedScrollTo.js -o dist/bundle.js",
    "prepublish": "npm run compile",
    "compile": "browserify src/AnimatedScrollTo.js -o dist/bundle.js -t [ babelify ]"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davesnx/scrollto-with-animation"
  },
  "bugs": {
    "url": "https://github.com/davesnx/scrollto-with-animation/issues"
  },
  "keywords": [
    "animation",
    "requestAnimationFrame",
    "cross-browser",
    "setTimeout",
    "easeInQuad",
    "browserify"
  ],
  "author": "David Sancho <dsnxmoreno@gmail.com> (https://github.com/davesnx)",
  "license": "MIT",
  "dependencies": {
    "animation-frame": "^0.2.5"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "watchify": "^3.7.0"
  }
}
